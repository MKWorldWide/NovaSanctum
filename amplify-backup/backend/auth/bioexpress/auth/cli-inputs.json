{
  "version": 1,
  "cognitoConfig": {
    "identityPoolName": "bioexpress_identitypool",
    "allowUnauthenticatedIdentities": false,
    "resourceNameTruncated": "bioexp",
    "userPoolName": "bioexpress_userpool",
    "autoVerifiedAttributes": [
      "email"
    ],
    "mfaConfiguration": "OFF",
    "mfaTypes": [
      "SMS Text Message"
    ],
    "smsAuthenticationMessage": "Your mystical verification code is {####}",
    "smsVerificationMessage": "Your mystical verification code is {####}",
    "emailVerificationSubject": "Your mystical verification code",
    "emailVerificationMessage": "Your mystical verification code is {####}",
    "defaultPasswordPolicy": true,
    "passwordPolicyMinLength": 12,
    "passwordPolicyCharacters": [
      "Requires Uppercase",
      "Requires Lowercase",
      "Requires Numbers",
      "Requires Symbols"
    ],
    "requiredAttributes": [
      "email",
      "name"
    ],
    "aliasAttributes": [],
    "userpoolClientGenerateSecret": false,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientWriteAttributes": [
      "email",
      "name"
    ],
    "userpoolClientReadAttributes": [
      "email",
      "name"
    ],
    "userpoolClientLambdaRole": "bioexp_userpoolclient_lambda_role",
    "userpoolClientSetAttributes": false,
    "sharedId": "bioexp",
    "resourceName": "bioexpress",
    "authSelections": "identityPoolAndUserPool",
    "useDefault": "manual",
    "userPoolGroups": false,
    "userPoolGroupList": [],
    "serviceName": "Cognito",
    "usernameCaseSensitive": false,
    "useEnabledMfas": true,
    "authRoleArn": {
      "Fn::GetAtt": [
        "AuthRole",
        "Arn"
      ]
    },
    "unauthRoleArn": {
      "Fn::GetAtt": [
        "UnauthRole",
        "Arn"
      ]
    },
    "breakCircularDependency": true,
    "dependsOn": []
  }
} 